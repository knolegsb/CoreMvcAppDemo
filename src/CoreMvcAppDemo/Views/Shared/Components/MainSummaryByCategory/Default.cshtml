@foreach (var m in Model)
{
    <div class="post_item">
        <div class="post_item_text">
            <span class="post_date">@m.PostDate.ToString("yyy-MM-dd")</span>
            <span class="post-title">
                <a href="/DotNetNote/Details/@m.Id">
                    @CoreMvcAppDemo.Dul.StringLibrary.CutStringUnicode(@m.Title, 33)
                </a>
            </span>
        </div>
    </div>
}
